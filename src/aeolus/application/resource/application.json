{
  "application": {
    "name": "aeolus",
    "version": "0.7"
  },
  "dobby": {
    "staticContent": {
      "directory": "static"
    },
    "session": {
      "age": 24
    },
    "port": 3333,
    "threads": 10,
    "mode": "http",
    "logLevel": "WARN"
  },
