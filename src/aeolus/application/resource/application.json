    "unauthorizedRedirectTarget": "/landing",
    "unauthorizedRedirectPaths": [
      "/",
      "/index.html",
      "/month",
      "/month/",
      "/month/index.html",
      "/year",
      "/year/",
      "/year/index.html",
      "/compare",
      "/compare/",
      "/compare/index.html",
      "/blanket",
      "/blanket/",
      "/blanket/index.html"
    ]
  }
